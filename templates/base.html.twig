<!DOCTYPE html>
{# @var dict \App\Lang\LanguagePack #}
{% set dict = localLang.dictionary %}

<html lang="{{ dict.localLang }}">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>
        {% block fullTitle %}
            {{ dict.websiteName }} - {% block title %} {{ dict.home }} {% endblock %}
        {% endblock %}
    </title>

    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Favicon -->
    <link rel="icon" href="{{ asset('assets/img/favicon.ico') }}">

    <!-- Vendor CSS -->
    <link href="{{ asset('lib/css/vendors.css') }}" rel="stylesheet">
    <!-- Main CSS -->
    <link href="{{ asset('lib/css/style.css') }}" rel="stylesheet">
</head>
<body>

{% embed 'layout/header.twig' %} {% endembed %}

{% block bodyFull %}
    {% block body %}
        It should work!
    {% endblock %}
{% endblock %}

{% embed 'layout/footer.twig' %} {% endembed %}

<!-- Vendor JS -->
<script src="{{ asset('lib/js/vendors.js') }}"></script>

<!-- Active JS -->
<script src="{{ asset('lib/js/active.js') }}"></script>
</body>

</html>